{
    "name": "library-backlog",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-feather": "^2.0.8"
    },
    "scripts": {
        "build": "webpack",
        "build:sass": "npm run copy-assets && node-sass src/styles/main.scss build/static/main.css",
        "lint": "eslint 'src/**/*.{js,ts,tsx}'",
        "lint:fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
        "copy-assets": "copyfiles -u 1 src/assets/**/* build",
        "watch:webpack": "webpack --watch",
        "watch:sass": "npm run copy-assets && node-sass -w src/styles/main.scss build/static/main.css"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/plugin-transform-runtime": "^7.11.5",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-react": "^7.10.4",
        "@babel/preset-typescript": "^7.10.4",
        "@babel/runtime": "^7.11.2",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.5.0",
        "@testing-library/user-event": "^7.2.1",
        "@types/jest": "^24.9.1",
        "@types/node": "^12.12.54",
        "@types/react": "^16.9.48",
        "@types/react-dom": "^16.9.8",
        "@types/webpack": "^4.41.22",
        "@typescript-eslint/eslint-plugin": "^4.4.1",
        "@typescript-eslint/parser": "^4.4.1",
        "babel-loader": "^8.1.0",
        "copyfiles": "^2.4.0",
        "eslint": "^7.11.0",
        "eslint-plugin-react": "^7.21.4",
        "eslint-plugin-react-hooks": "^4.1.2",
        "node-sass": "^4.14.1",
        "ts-node": "^9.0.0",
        "typescript": "^3.7.5",
        "webpack": "^5.1.0",
        "webpack-cli": "^4.0.0"
    }
}
